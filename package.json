{
  "name": "icarus",
  "version": "1.1.3",
  "productName": "Icarus Comic Reader",
  "description": "小薄本 - 单手看漫画",
  "main": "main.js",
  "scripts": {
    "dist": "sh dist.sh",
    "start": "npm run dist && electron dist",
    "debug": "npm run dist && electron dist --debug",
    "build": "npm run dist -- --install && npm run build:osx",
    "build:osx": "electron-packager dist Icarus --platform=darwin --arch=x64 --version=0.35.4 --out=release --overwrite"
  },
  "author": "otakustay",
  "license": "LGPL-3.0",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "babel-polyfill": "^6.2.0",
    "datauri": "^0.8.0",
    "denodeify": "^1.2.1",
    "etpl": "^3.0.1",
    "glob": "^6.0.1",
    "log4js": "^0.6.29",
    "mkdirp": "^0.5.1",
    "moment": "^2.10.6",
    "nedb-promise": "^2.0.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "electron-packager": "^7.3.0",
    "electron-prebuilt": "^1.3.1"
  }
}
