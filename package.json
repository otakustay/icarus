    "element-resize-detector": "^1.1.11",
    "glob": "^6.0.1",
    "iconv-lite": "^0.4.15",
    "image-size": "^0.5.1",
    "jschardet": "^1.4.2",
    "log4js": "^0.6.29",
    "mkdirp": "^0.5.1",
    "moment": "^2.10.6",
    "nedb-promise": "^2.0.0",
    "pinyin": "^2.8.0",
