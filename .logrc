{
    "appenders": [
        {
            "type": "console"
        },
        {
            "type": "logLevelFilter",
            "level": "INFO",
            "appender": {
                "type": "file",
                "filename": "trace.log",
                "maxLogSize": 2097152,
                "backups": 3
            }
        },
        {
            "type": "logLevelFilter",
            "level": "WARN",
            "appender": {
                "type": "file",
                "filename": "wf.log",
                "maxLogSize": 2097152,
                "backups": 3
            }
        }
    ],
    "levels": {
        "[all]": "ALL"
    }
}
